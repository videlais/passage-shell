{
  "name": "passage-shell",
  "version": "1.2.1",
  "description": "Passage Shell is a browser shell for running Twine projects.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "jest --coverage"
  },
  "repository": "",
  "keywords": [
    "Electron",
    "Twine"
  ],
  "author": "Dan Cox",
  "license": "MIT",
  "devDependencies": {
    "electron": "^38.2.0",
    "express": "^5.1.0",
    "jest": "^30.2.0",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  }
}
