{
  "name": "passage-shell",
  "version": "1.2.1",
  "description": "Passage Shell is a browser shell for running Twine projects.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "all": "npm run lint && npm run test"
  },
  "repository": "",
  "keywords": [
    "Electron",
    "Twine"
  ],
  "author": "Dan Cox",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "electron": "^39.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-jsdoc": "^61.2.1",
    "express": "^5.1.0",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "dependencies": {
    "express-rate-limit": "^8.2.1"
  }
}
